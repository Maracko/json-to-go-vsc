{
	"name": "json-to-go",
	"author": {
		"name": "Mario Petriƒçko",
		"email": "mario.petricko@outlook.com"
	},
	"publisher": "maracko",
	"repository": "https://github.com/Maracko/json-to-go-vsc",
	"displayName": "JSON to Go",
	"description": "Convert JSON data to Go struct",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.55.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-to-go.convert",
				"title": "JSON to Go"
			}
		],
		"menus": {
			"editor/context": [
				{
					"when": "editorLangId in json-to-go:supportedLanguages",
					"command": "json-to-go.convert"
				}
			]
		},
		"configuration": {
			"title": "JSON to Go",
			"properties": {
				"json-to-go.inlineTypeDefinitions": {
					"type": "boolean",
					"default": false,
					"description": "Toggle to show nested structs in-line or below the main struct"
				},
				"json-to-go.contextMenu.supportedLanguages": {
					"type": "array",
					"default": [
						"javascript",
						"typescript",
						"json",
						"jsonc",
						"html",
						"go"
					],
					"description": "For which languages do you want to show JSON to Go command in context menu"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.55.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"tmp": "^0.2.1"
	},
	"icon": "icon.png"
}